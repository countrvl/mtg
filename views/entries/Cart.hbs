
<div id="listCard">
    <div id="cards">
        <h2>Добавленные карты:</h2>
    <button name="btnCheck" id="check" type="button" class=" text-center p-3 btn btn-outline-light fs-4">Купить</button>
        
    </div>
<div id="body" class="buycards">
   {{#each addCards}}
    <div id="box">
      <div id="title">
            <h3>{{'Card.title'}}</h3>
        </div>
        <div class="img">
            <img src="{{'Card.img'}}" alt="">
        </div>
          <div id="ganre">
            <p>Цена: {{'Card.price'}} $</p>
            <p>Состояние: {{'Card.Сondition.condition_name'}}</p>
            <p>Город продавца: {{'Card.User.City.city_name'}}</p>
        </div>
        <div>
        <button name="btnDel" type="button" id={{'Card.id'}} class="btn btn-outline-danger">Delete</button>
        </div>
    </div> 
     {{/each}} 
    </div>
    </div>

   