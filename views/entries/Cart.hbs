<div class="space" ></div>
    <div id="kor">
        <div class="space2" ></div>
    <div id="cards">
        <h2>Добавленные карточки:</h2>
    </div>

<div id="body" class="buycards">
   {{#each addCards}}
    <div id="box">
      <div id="title">
            <h3>{{'Card.title'}}</h3>
        </div>
        <div class="img">
            <img src="{{'Card.img'}}" alt="">
        </div>
          <div id="ganre">
            <p>Цена: {{'Card.price'}} $</p>
            <p>Состояние: {{'Card.Сondition.condition_name'}}</p>
            <p>Город продавца: {{'Card.User.City.city_name'}}</p>
        </div>
        <div>
        <button name="btnDel" type="button" id={{'Card.id'}} class="btn btn-outline-danger">Удалить</button>
        </div>
    </div> 
     {{/each}} 
    </div>

     <div class="d-flex justify-content-center d-grid gap-2">
  <button class="btn btn-success w-50 p-3 btn-lg" name="btnCheck" id="check" type="button">Купить</button>
</div>
    </div>

   